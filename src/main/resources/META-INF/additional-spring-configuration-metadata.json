        {
          "value": "Key_Shared",
          "description": "按键共享订阅，相同键的消息发送给同一个消费者"
        }
      ]
    },
    {
      "name": "spring.pulsar.consumer.subscription-initial-position",
      "values": [
        {
          "value": "Earliest",
          "description": "从最早的消息开始消费"
        },
        {
          "value": "Latest",
          "description": "从最新的消息开始消费"
        }
      ]
    }
  ]
}